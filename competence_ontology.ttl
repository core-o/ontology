#################################################################
#    Classes
#################################################################

###  http://example.com#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf gufo:FunctionalComplex .


###  http://example.com#Artifact
:Artifact rdf:type owl:Class ;
          rdfs:subClassOf gufo:FunctionalComplex .


###  http://example.com#ArtifactType
:ArtifactType rdf:type owl:Class ;
              rdfs:subClassOf :SubstantialType .


###  http://example.com#Attitude
:Attitude rdf:type owl:Class ;
          rdfs:subClassOf :Disposition ,
                          :HumanAspect .


###  http://example.com#AttitudeType
:AttitudeType rdf:type owl:Class ;
              rdfs:subClassOf :HumanAspectType .


###  http://example.com#CapabilityContext
:CapabilityContext rdf:type owl:Class ;
                   rdfs:subClassOf gufo:Situation .


###  http://example.com#CapabilityManifestation
:CapabilityManifestation rdf:type owl:Class ;
                         rdfs:subClassOf gufo:Event ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :capabilityManifestationBroughtAboutCapabilityOutcome ;
                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass :CapabilityOutcome
                                         ] .


###  http://example.com#CapabilityOutcome
:CapabilityOutcome rdf:type owl:Class ;
                   rdfs:subClassOf gufo:Situation ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty [ owl:inverseOf :capabilityManifestationBroughtAboutCapabilityOutcome
                                                    ] ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :CapabilityManifestation
                                   ] .


###  http://example.com#CapabilityRequiringRole
:CapabilityRequiringRole rdf:type owl:Class ;
                         rdfs:subClassOf gufo:Role .


###  http://example.com#CapabilityType
:CapabilityType rdf:type owl:Class ;
                rdfs:subClassOf :DispositionType .


###  http://example.com#CompetenceContext
:CompetenceContext rdf:type owl:Class ;
                   rdfs:subClassOf gufo:Situation ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :competenceContextContributedToTriggerPersonalCompetence ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :PersonalCompetence
                                   ] .


###  http://example.com#CompetenceType
:CompetenceType rdf:type owl:Class ;
                rdfs:subClassOf :HumanCapabilityType .


###  http://example.com#Disposition
:Disposition rdf:type owl:Class ;
             rdfs:subClassOf gufo:IntrinsicMode .


###  http://example.com#DispositionType
:DispositionType rdf:type owl:Class ;
                 rdfs:subClassOf :MomentType .


###  http://example.com#Evidence
:Evidence rdf:type owl:Class ;
          rdfs:subClassOf gufo:FunctionalComplex .


###  http://example.com#Field
:Field rdf:type owl:Class ;
       rdfs:subClassOf gufo:ConcreteIndividualType .


###  http://example.com#HumanAspect
:HumanAspect rdf:type owl:Class ;
             rdfs:subClassOf gufo:IntrinsicMode ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :inheresIn_1 ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :Person
                             ] .


###  http://example.com#HumanAspectPhase
:HumanAspectPhase rdf:type owl:Class ;
                  rdfs:subClassOf :HumanAspectType ,
                                  gufo:Phase .


###  http://example.com#HumanAspectType
:HumanAspectType rdf:type owl:Class ;
                 rdfs:subClassOf :MomentType .


###  http://example.com#HumanCapability
:HumanCapability rdf:type owl:Class ;
                 rdfs:subClassOf :Disposition ,
                                 :HumanAspect ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :humanCapabilityInheresInProficiency ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :Proficiency
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :inheresIn ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :Person
                                 ] .


###  http://example.com#HumanCapabilityType
:HumanCapabilityType rdf:type owl:Class ;
                     rdfs:subClassOf :CapabilityType ,
                                     :DispositionType ,
                                     :HumanAspectType .


###  http://example.com#HumanCharacteristic
:HumanCharacteristic rdf:type owl:Class ;
                     rdfs:subClassOf :HumanAspect .


###  http://example.com#HumanQuality
:HumanQuality rdf:type owl:Class ;
              rdfs:subClassOf :HumanCharacteristic .


###  http://example.com#HumanTask
:HumanTask rdf:type owl:Class ;
           rdfs:subClassOf gufo:Event ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty [ owl:inverseOf :artifactParticipatedInHumanTask
                                            ] ;
                             owl:someValuesFrom :Artifact
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty [ owl:inverseOf :performs
                                            ] ;
                             owl:someValuesFrom :Person
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty [ owl:inverseOf :resourceParticipatedInHumanTask
                                            ] ;
                             owl:someValuesFrom :Resource
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty [ owl:inverseOf :taskOutcomeBroughtAboutHumanTask
                                            ] ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :TaskOutcome
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty [ owl:inverseOf :taskOutputWasCreatedInHumanTask
                                            ] ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :TaskOutput
                           ] .


###  http://example.com#HumanTrait
:HumanTrait rdf:type owl:Class ;
            rdfs:subClassOf :HumanCharacteristic .


###  http://example.com#Knowledge
:Knowledge rdf:type owl:Class ;
           rdfs:subClassOf :Disposition ,
                           :HumanAspect .


###  http://example.com#KnowledgeType
:KnowledgeType rdf:type owl:Class ;
               rdfs:subClassOf :HumanAspectType .


###  http://example.com#MomentType
:MomentType rdf:type owl:Class ;
            rdfs:subClassOf gufo:EndurantType .


###  http://example.com#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf :Agent ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty [ owl:inverseOf :inheresIn
                                         ] ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :HumanCapability
                        ] .


###  http://example.com#PersonalCompetence
:PersonalCompetence rdf:type owl:Class ;
                    rdfs:subClassOf :HumanCapability ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty [ owl:inverseOf :competenceContextContributedToTriggerPersonalCompetence
                                                     ] ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :CompetenceContext
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty [ owl:inverseOf :isManifestedBy
                                                     ] ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass :HumanCapability
                                    ] .


###  http://example.com#Proficiency
:Proficiency rdf:type owl:Class ;
             rdfs:subClassOf gufo:IntrinsicMode ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty [ owl:inverseOf :humanCapabilityInheresInProficiency
                                              ] ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :HumanCapability
                             ] .


###  http://example.com#Resource
:Resource rdf:type owl:Class ;
          rdfs:subClassOf gufo:FunctionalComplex .


###  http://example.com#ResourceType
:ResourceType rdf:type owl:Class ;
              rdfs:subClassOf :SubstantialType .


###  http://example.com#Skill
:Skill rdf:type owl:Class ;
       rdfs:subClassOf :HumanCapability .


###  http://example.com#SkillType
:SkillType rdf:type owl:Class ;
           rdfs:subClassOf :HumanCapabilityType .


###  http://example.com#SubstantialType
:SubstantialType rdf:type owl:Class ;
                 rdfs:subClassOf gufo:EndurantType .


###  http://example.com#TaskInput
:TaskInput rdf:type owl:Class ;
           rdfs:subClassOf :Artifact .


###  http://example.com#TaskOutcome
:TaskOutcome rdf:type owl:Class ;
             rdfs:subClassOf gufo:Situation ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :taskOutcomeBroughtAboutHumanTask ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :HumanTask
                             ] .


###  http://example.com#TaskOutput
:TaskOutput rdf:type owl:Class ;
            rdfs:subClassOf :Artifact ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :taskOutputWasCreatedInHumanTask ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :HumanTask
                            ] .


###  http://example.com#TaskType
:TaskType rdf:type owl:Class ;
          rdfs:subClassOf gufo:EventType .


